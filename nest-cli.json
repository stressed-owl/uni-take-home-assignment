{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/uni-take-home-assignment/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/uni-take-home-assignment/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/uni-take-home-assignment",
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "fb-collector": {
      "type": "application",
      "root": "apps/fb-collector",
      "entryFile": "main",
      "sourceRoot": "apps/fb-collector/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fb-collector/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "reporter": {
      "type": "application",
      "root": "apps/reporter",
      "entryFile": "main",
      "sourceRoot": "apps/reporter/src",
      "compilerOptions": {
        "tsConfigPath": "apps/reporter/tsconfig.app.json"
      }
    },
    "ttk-collector": {
      "type": "application",
      "root": "apps/ttk-collector",
      "entryFile": "main",
      "sourceRoot": "apps/ttk-collector/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ttk-collector/tsconfig.app.json"
      }
    },
    "uni-take-home-assignment": {
      "type": "application",
      "root": "apps/uni-take-home-assignment",
      "entryFile": "main",
      "sourceRoot": "apps/uni-take-home-assignment/src",
      "compilerOptions": {
        "tsConfigPath": "apps/uni-take-home-assignment/tsconfig.app.json"
      }
    }
  }
}